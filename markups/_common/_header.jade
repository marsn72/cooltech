header.header        .header__one            .container                ul.header__socials                        each social in ['fb', 'vk', 'tw']                            li(class="header__socials-item header__socials-item_" + social)                                a(href="#").header__socials-link= social                .header__adress ул. Маяковская 6, офис 17                a(href="tel.: +79211231213").header__phone                    span.header__phone-number +7 (921) 123-12-13        .header__two            .container                a(href="#").header__logo                    img(src="img/logo.png" alt="Интернет-магазин Cooltech").header__logo-img                .header__search                    form(action="#" method="get").header__search-form                        input(type="text" placeholder="Что будем искать").header__search-input                        input(type="submit" value="Найти").header__search-submit                ul.header__menu                    li.header__menu-item                        a(href="#").header__menu-link Карта проезда                    li.header__menu-item                        a(href="#").header__menu-link История заказов                    li.header__menu-item                        a(href="#").header__menu-link Корзина